(define (problem menuext3b-tc3)
          (:domain menuext3b)

          (:objects
            dilluns dimarts dimecres dijous divendres DummyD - dia
escalivada_verdures crema_de_carabasso raviolis_ricotta truita_potatge brocoli_allada fajitas_verdures amanida_de_cogombre crema_espinacs verdures_a_la_planxa sopa_miso - primer
sopa_de_galets cuixa_ànec crema_catalana hamburguesa_mongeta estofat_soja truita_verdures salmo_grillat curry_verdures xai_rostit wok_verdures - segon
DummyT amanida carn crema llegums pasta peix sopa verdures - tipus
          )

          (:init
            (incompatible crema_espinacs cuixa_ànec)
    (incompatible truita_potatge salmo_grillat)
    (incompatible crema_de_carabasso cuixa_ànec)
    (incompatible escalivada_verdures xai_rostit)
    (incompatible amanida_de_cogombre salmo_grillat)
    (incompatible sopa_miso truita_verdures)
    (incompatible crema_espinacs salmo_grillat)
    (incompatible crema_de_carabasso salmo_grillat)
    (platTipus escalivada_verdures verdures)
    (platTipus crema_de_carabasso crema)
    (platTipus raviolis_ricotta pasta)
    (platTipus truita_potatge llegums)
    (platTipus brocoli_allada verdures)
    (platTipus fajitas_verdures verdures)
    (platTipus amanida_de_cogombre amanida)
    (platTipus crema_espinacs crema)
    (platTipus verdures_a_la_planxa verdures)
    (platTipus sopa_miso sopa)
    (platTipus sopa_de_galets sopa)
    (platTipus cuixa_ànec carn)
    (platTipus crema_catalana crema)
    (platTipus hamburguesa_mongeta llegums)
    (platTipus estofat_soja llegums)
    (platTipus truita_verdures verdures)
    (platTipus salmo_grillat peix)
    (platTipus curry_verdures verdures)
    (platTipus xai_rostit carn)
    (platTipus wok_verdures verdures)
    (diaAnt DummyD dilluns)
    (diaAnt dilluns dimarts)
    (diaAnt dimarts dimecres)
    (diaAnt dimecres dijous)
    (diaAnt dijous divendres)
    (primerAsignat DummyD)
(segonAsignat  DummyD)
(diaPrimerTipus DummyD DummyT)
(diaSegonTipus  DummyD DummyT)
    (platObligatori amanida_de_cogombre dimarts)
    (platObligatori curry_verdures divendres)
          )

          (:goal
            (forall (?d - dia)
              (and (primerAsignat ?d) (segonAsignat ?d)))
          )
        )