DummyT amanida carn crema llegums pasta peix sopa verdures - tipus
          )

          (:init
            (incompatible cigrons_saltejats llenguado_amb_lima)
    (incompatible tallarins_carbonara crema_de_xocolata)
    (incompatible cigrons_saltejats bacalla_amb_tomaquet)
    (incompatible crema_de_bolets bacalla_amb_tomaquet)
    (incompatible amanida_asiatica bacalla_amb_tomaquet)
    (incompatible tallarins_carbonara crema_catalana)
    (incompatible penne_arrabiata crema_avellana)
    (incompatible penne_arrabiata crema_de_xocolata)
    (platTipus crema_de_bolets crema)
    (platTipus sopa_de_peix sopa)
    (platTipus penne_arrabiata pasta)
    (platTipus estofat_llenties llegums)
    (platTipus tallarins_carbonara pasta)
    (platTipus sopa_miso sopa)
    (platTipus escalivada_verdures verdures)
    (platTipus cigrons_saltejats llegums)
    (platTipus amanida_asiatica amanida)
    (platTipus macarrons_forn pasta)
    (platTipus crema_avellana crema)
    (platTipus llenguado_amb_lima peix)
    (platTipus tataki_verdures verdures)
    (platTipus estofat_cigrons llegums)
    (platTipus pollastre_a_la_planxa carn)
    (platTipus bacalla_amb_tomaquet peix)
    (platTipus crema_de_xocolata crema)
    (platTipus crema_catalana crema)
    (platTipus sopa_ceba sopa)
