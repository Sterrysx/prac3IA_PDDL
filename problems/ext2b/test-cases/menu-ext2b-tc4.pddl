(define (problem menuext2b-tc4)
          (:domain menuext2b)

          (:objects
            dilluns dimarts dimecres dijous divendres DummyD - dia
escalivada_verdures hummus crema_de_bolets sopa_de_cigrons macarrons_forn amanida_de_tomaquet sopa_de_marisc truita_potatge amanida_fasols raviolis_ricotta - primer
orata_forno sopa_de_peix crema_taronja cuixa_anec bacalla_amb_tomaquet estofat_cigrons crema_de_xocolata sopa_ceba wok_verdures crema_catalana - segon
DummyT amanida carn crema llegums pasta peix sopa verdures - tipus
          )

          (:init
            (incompatible amanida_de_tomaquet orata_forno)
    (incompatible escalivada_verdures sopa_de_peix)
    (incompatible macarrons_forn crema_taronja)
    (incompatible raviolis_ricotta crema_catalana)
    (incompatible amanida_fasols bacalla_amb_tomaquet)
    (incompatible amanida_de_tomaquet bacalla_amb_tomaquet)
    (incompatible hummus orata_forno)
    (incompatible sopa_de_cigrons wok_verdures)
    (platTipus escalivada_verdures verdures)
    (platTipus hummus llegums)
    (platTipus crema_de_bolets crema)
    (platTipus sopa_de_cigrons sopa)
    (platTipus macarrons_forn pasta)
    (platTipus amanida_de_tomaquet amanida)
    (platTipus sopa_de_marisc sopa)
    (platTipus truita_potatge llegums)
    (platTipus amanida_fasols llegums)
    (platTipus raviolis_ricotta pasta)
    (platTipus orata_forno peix)
    (platTipus sopa_de_peix sopa)
    (platTipus crema_taronja crema)
    (platTipus cuixa_anec carn)
    (platTipus bacalla_amb_tomaquet peix)
    (platTipus estofat_cigrons llegums)
    (platTipus crema_de_xocolata crema)
    (platTipus sopa_ceba sopa)
    (platTipus wok_verdures verdures)
    (platTipus crema_catalana crema)
    (diaAnt DummyD dilluns)
    (diaAnt dilluns dimarts)
    (diaAnt dimarts dimecres)
    (diaAnt dimecres dijous)
    (diaAnt dijous divendres)
    (primerAsignat DummyD)
(segonAsignat  DummyD)
(diaPrimerTipus DummyD DummyT)
(diaSegonTipus  DummyD DummyT)
          )

          (:goal
            (forall (?d - dia)
              (and (primerAsignat ?d) (segonAsignat ?d)))
          )
        )